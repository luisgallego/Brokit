 {
  "manifest_version": 2,

  "name": "Robrock",
  "version": "1.0.0",
  "description": "Wii",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "background.html"
  },
  "content_scripts": [ {
    "js": [ "jquery.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": [
    "tabs", "http://www.google.com/*"
  ]
}